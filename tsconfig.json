{
    "extends": "astro/tsconfigs/strict",
    "include": [
        ".astro/types.d.ts",
        "**/*",
        "src/types/*.d.ts"
    ],
    "exclude": [
        "dist"
    ],
    "compilerOptions": {
        "strictNullChecks": true,
        "jsx": "react-jsx",
        "jsxImportSource": "preact",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "paths": {
            "react": [
                "./node_modules/preact/compat/"
            ],
            "react-dom": [
                "./node_modules/preact/compat/"
            ]
        }
    }
}
